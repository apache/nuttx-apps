config NETUTILS_MQTTC
	tristate "Enable MQTT-C"
	default n
	---help---
		Enable MQTT-C

if NETUTILS_MQTTC

config NETUTILS_MQTTC_WITH_MBEDTLS
	bool "Enable MQTT-C with mbedtls"
	default n

config NETUTILS_MQTTC_VERSION
	string "MQTT-C Version"
	default "1.1.5"

endif
