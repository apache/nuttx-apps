config TESTS_PTHREAD
	tristate "vela pthread test"
	default n
	---help---
		Enable pthread test for the vela

if TESTS_PTHREAD

config TESTS_PTHREAD_PRIORITY
	int "PTHREAD task priority"
	default 99

config TESTS_PTHREAD_STACKSIZE
	int "PTHREAD stack size"
	default 16384


config CM_PTHREAD_TEST
	bool "enbale pthread test"
	default n
	depends on TESTING_CMOCKA


endif # TESTS_PTHREAD