config TESTS_SCHED
	tristate "vela sched test"
	default n
	---help---
		Enable sched test for the vela

if TESTS_SCHED


config TESTS_SCHED_PRIORITY
	int "sched task priority"
	default 99

config TESTS_SCHED_STACKSIZE
	int "sched stack size"
	default 16384

config CM_SCHED_TEST
	bool "enbale schedule test"
	default n
	depends on TESTING_CMOCKA


endif # TESTS_SCHED
