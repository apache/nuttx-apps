config TESTS_SOCKET
	tristate "vela socket test"
	default n
	---help---
		Enable socket test for the vela

if TESTS_SOCKET


config TESTS_SOCKET_PRIORITY
	int "socket task priority"
	default 99

config TESTS_SOCKET_STACKSIZE
	int "socket stack size"
	default 16384

config CM_SOCKET_TEST
	bool "enbale socket test"
	default n
	depends on TESTING_CMOCKA


endif # TESTS_SOCKET
